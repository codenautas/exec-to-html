{
  "name": "exec-to-html",
  "description": "Execute command sending output through stream to client",
  "version": "0.1.0",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "files": ["bin"],
  "license": "MIT",
  "repository": "codenautas/exec-to-html",
  "contributors": [{
    "name": "Emilio Platzer", "email": "emilioplatzer@gmail.com"
  }],
  "dependencies": {
    "iconv-lite": "~0.4.13",
    "lodash": "~4.11.1",
    "read-yaml-promise": "~1.0.2",
    "send": "~0.13.2",

    "best-promise": "~0.2.4",
    "mini-tools": "~0.2.8"
  },
  "devDependencies": {
    "body-parser": "~1.15.0",
    "colors": "~1.1.2",
    "cookie-parser": "~1.4.1",
    "expect.js": "~0.3.1",
    "express": "~4.13.4",
    "express-session": "~1.13.0",
    "fs-promise": "~0.5.0", 
    "istanbul": "~0.4.3",
    "jade": "~1.11.0",
    "mocha": "~2.4.5",
    "moment": "~2.13.0",
    "semver": "~5.1.0",
    "supertest": "1.2.0",

    "ajax-best-promise": "~0.0.7",
    "extension-serve-static": "0.1.6"
  },
  "main": "bin/exec-to-html.js",
  "engines": {
    "node": ">= 0.12.0"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node examples/server/server.js"
  },
  "jshintConfig": {
    "asi": false,
    "curly": true,
    "forin": true
  },
  "eslintConfig": {
    "env": {
      "node": false
    },
    "rules": {
      "strict": 0,
      "no-console": 1,
      "no-unused-vars": 1
    }
  }, 
  "qa-control": {
    "package-version": "0.2.0",
    "run-in": "server",
    "test-appveyor": true,
    "type": "lib",
    "stage": "extending",
    "coverage": 80
  }
}
